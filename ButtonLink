        function openRickroll() {
            var confirmLeave = confirm("You are about to leave this page. Do you want to continue?");
            if (confirmLeave) {
                var videoUrl = "https://www.youtube.com/watch?v=dQw4w9WgXcQ";
                window.open(videoUrl);
            }
        }
